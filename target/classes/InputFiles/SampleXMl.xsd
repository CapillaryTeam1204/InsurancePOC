<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="policyPeriod" type="policyPeriodType"/>
  <xs:complexType name="coverageTermsType">
    <xs:sequence>
      <xs:element name="termName">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="DeductibleAmount"/>
            <xs:enumeration value="LimitPerPersonAmount"/>
            <xs:enumeration value="BodilyInjuryLimitPerPersonAmount"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="termValue">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="500"/>
            <xs:enumeration value="5000"/>
            <xs:enumeration value="25000"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="personalCoveragesType">
    <xs:sequence>
      <xs:element name="patternCode">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="Comprehensive"/>
            <xs:enumeration value="Collision"/>
            <xs:enumeration value="MedicalPayments"/>
            <xs:enumeration value="UninsuredBodilyInjury"/>
            <xs:enumeration value="BodilyInjuryAndPropertyDamage"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element type="coverageTermsType" name="coverageTerms" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="vehicleRegisteredOwnersType">
    <xs:sequence>
      <xs:element type="xs:string" name="clientIdentifier"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="garageLocationType">
    <xs:sequence>
      <xs:element type="xs:string" name="addressLine1"/>
      <xs:element type="xs:string" name="city"/>
      <xs:element type="xs:string" name="cityCode"/>
      <xs:element type="xs:string" name="countyCode"/>
      <xs:element type="xs:string" name="countyName"/>
      <xs:element type="xs:string" name="latitude"/>
      <xs:element type="xs:string" name="locationMatchCode"/>
      <xs:element type="xs:string" name="locationQualityCode"/>
      <xs:element type="xs:string" name="longitude"/>
      <xs:element type="xs:string" name="postalCode"/>
      <xs:element type="xs:string" name="state"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="personalVehiclesType">
    <xs:sequence>
      <xs:element type="personalCoveragesType" name="personalCoverages" maxOccurs="unbounded" minOccurs="0"/>
      <xs:element type="xs:string" name="isDssEnrolled"/>
      <xs:element type="xs:string" name="totalCustomizationAmount"/>
      <xs:element type="xs:string" name="primaryUse"/>
      <xs:element type="xs:string" name="vin"/>
      <xs:element type="xs:string" name="year"/>
      <xs:element type="xs:string" name="bodyType"/>
      <xs:element type="xs:string" name="model"/>
      <xs:element type="xs:string" name="make"/>
      <xs:element type="xs:string" name="vehicleSafetyDiscountCode"/>
      <xs:element type="xs:string" name="collisionIRGCode"/>
      <xs:element type="xs:string" name="comprehensiveIRGCode"/>
      <xs:element type="xs:string" name="publicID"/>
      <xs:element type="xs:string" name="customVehicleSwitch"/>
      <xs:element type="vehicleRegisteredOwnersType" name="vehicleRegisteredOwners"/>
      <xs:element type="garageLocationType" name="garageLocation"/>
      <xs:element type="xs:string" name="kitCarIndicator"/>
      <xs:element type="xs:string" name="transportationNetworkCompanyIndicator"/>
      <xs:element type="xs:string" name="transportationIncidentalOccupationDutyIndicator"/>
      <xs:element type="xs:string" name="transportationSchoolChurchEmployeeIndicator"/>
      <xs:element type="xs:string" name="vehicleTenureDate"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="policyDriversType">
    <xs:sequence>
      <xs:element type="xs:string" name="dateOfBirth"/>
      <xs:element type="xs:string" name="clientIdentifier"/>
      <xs:element type="xs:string" name="subType"/>
      <xs:element type="xs:string" name="firstName"/>
      <xs:element type="xs:string" name="lastName"/>
      <xs:element type="xs:string" name="militaryDriverSwitch"/>
      <xs:element type="xs:string" name="militaryFrequentVehicleAccessSwitch"/>
      <xs:element type="xs:string" name="gender"/>
      <xs:element type="xs:string" name="originalLicenseDate"/>
      <xs:element type="xs:string" name="licensedType"/>
      <xs:element type="xs:string" name="employeeDriverSwitch"/>
      <xs:element type="xs:string" name="driverTenureStartDate"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="periodStartType">
    <xs:sequence>
      <xs:element type="xs:string" name="publicID" minOccurs="0"/>
      <xs:element type="personalVehiclesType" name="personalVehicles" minOccurs="0"/>
      <xs:element type="personalCoveragesType" name="personalCoverages" maxOccurs="unbounded" minOccurs="0"/>
      <xs:element type="policyDriversType" name="policyDrivers" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="policyPeriodType">
    <xs:choice maxOccurs="unbounded" minOccurs="0">
      <xs:element type="xs:string" name="periodStart"/>
      <xs:element name="tracsactionType">
        <xs:complexType mixed="true">
          <xs:sequence>
            <xs:element type="xs:string" name="publicID" minOccurs="0"/>
            <xs:element type="personalVehiclesType" name="personalVehicles" minOccurs="0"/>
            <xs:element type="personalCoveragesType" name="personalCoverages" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="policyDriversType" name="policyDrivers" minOccurs="0"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:complexType>
</xs:schema>